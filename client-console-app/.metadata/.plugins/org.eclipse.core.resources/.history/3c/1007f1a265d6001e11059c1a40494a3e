package command;

import java.io.IOException;

import account.UserAccount;
import service.UserAccountService;

public class AvailableBalanceCommand implements Command {

	private final UserAccountService accountService;
	
	public AvailableBalanceCommand(UserAccountService accountService) {
		this.accountService = accountService;
	}
	
	@Override
	public void execute(UserAccount account) throws NumberFormatException, IOException {
		double balance = accountService.getBalance(Integer.parseInt(account.getCardNumber()));
		
	}

}
